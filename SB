local function Aura()
  while true do
        local players = game:GetService("Players"):GetPlayers()
        for _, player in ipairs(players) do
            local character = player.Character
            if character then
                local leftArm = character:FindFirstChild("Left Arm")
                if leftArm then
                    local args = {
                        [1] = leftArm
                    }
                    game:GetService("ReplicatedStorage").DiceHit:FireServer(unpack(args))
                end
            end
        end
        wait(0.75)
    end
end
  coroutine.wrap(Aura)()
if game.Players.LocalPlayer.leaderstats.Slaps.Value >= 666 then
fireclickdetector(game.workspace.Lobby.Ghost.ClickDetector)
wait(0.3)
game:GetService("ReplicatedStorage").Ghostinvisibilityactivated:FireServer()  
  if game.Players.LocalPlayer._unlockedGloves.Dice.Value == true then
 fireclickdetector(game.workspace.Lobby.Dice.ClickDetector)   
