-- GUI
local platform = Instance.new("Part") platform.Name = 'safespot' platform.Size = Vector3.new(2048, 1, 2048) platform.Position = Vector3.new(-24058.8594, 306.104187, -844.946045) platform.CFrame = CFrame.new(platform.Position) platform.BrickColor = BrickColor.new("Bright blue") platform.Material = Enum.Material.Plastic platform.Transparency = 1 platform.Parent = workspace local weld = Instance.new("WeldConstraint") weld.Parent = platform weld.Part0 = platform weld.Part1 = workspace.Terrain
local Namecall
Namecall = hookmetamethod(game, "__namecall", function(self, ...)
   if getnamecallmethod() == "FireServer" and tostring(self) == "Ban" then
       return
   elseif getnamecallmethod() == "FireServer" and tostring(self) == "WalkSpeedChanged" then
       return
   elseif getnamecallmethod() == "FireServer" and tostring(self) == "AdminGUI" then
       return
   end
   return Namecall(self, ...)
end)
-- other
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/Dusty1234567890/Orion/main/Orion10')))()
local Window = OrionLib:MakeWindow({IntroText = 'Yet <font color="rgb(30, 30, 30)">Darker</font>', IntroIcon = "rbxassetid://16795945989", Name = "GASTER HUB | V 0.2", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
local anm = game:service'CoreGui':WaitForChild'Orion':GetChildren()[2]:WaitForChild'TopBar'.TextLabel
game:service'TweenService':Create(anm, TweenInfo.new(5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true, 0), {TextTransparency = 0.9}):Play()
local function titles()
for i = 1, string.len('GASTER HUB | V 0.2'), 1 do
anm.Text = string.sub('GASTER HUB | V 0.2', 1, i)
wait(.5)
end
wait(math.random(1, 4))
for i = string.len('GASTER HUB | V 0.2'), 0, -1 do
anm.Text = string.sub('GASTER HUB | V 0.2', 1, i)
wait(.04)
end
wait(math.random(1, 4))
return titles()
end
coroutine.wrap(titles)()
