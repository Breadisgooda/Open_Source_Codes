local function fireForPlayer(player)
    local args = {player}
    ReplicatedStorage.SM:FireServer(unpack(args))
end
for _, player in ipairs(Players:GetPlayers()) do
    fireForPlayer(player)
end
Players.PlayerAdded:Connect(fireForPlayer)
