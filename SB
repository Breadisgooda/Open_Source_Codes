para1 = Tab1:AddParagraph("Slap Aura's Current Speed", "every 0.9 sec")
game:GetService('RunService').RenderStepped:Connect(function()
if _G.speeder then
para1:Set('every '.. _G.speeder ..' sec')
else
para1:Set('every 0.9 sec')
end
end)
Aura1 = Tab1:AddToggle({
	Name = "Slap Aura",
	Default = false,
	Callback = function(Value)
	  if Value == true then
_G.SlapAura = true
local function SlapAura()
  while _G.SlapAura do
    for _, v in pairs(game:GetService('Players'):GetPlayers()) do
      if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild('InLobby') == nil and v.Character and v.Character:FindFirstChild('HumanoidRootPart') and v.Character:FindFirstChild('rock') == nil and v.Character:FindFirstChild('ded') == nil then
        if v.Character:FindFirstChild('Head') and v.Character:FindFirstChild('Head'):FindFirstChild('UnoReverseCard') == nil then
          if SlapList[game.Players.LocalPlayer.leaderstats.Glove.Value] then
          SlapList[game.Players.LocalPlayer.leaderstats.Glove.Value]:FireServer(v.Character:WaitForChild('HumanoidRootPart'))
          else
         Aura1:Set(false)
            Send('Info', 'Glove Is Not Supported', 'rbxassetid://17105411523', 7)
            Send('Info', 'S Aura Turned Off', 'rbxassetid://17105411523', 5)
          end
        end
      end
    end
    task.wait(_G.speeder or 0.9)
  end
end
coroutine.wrap(SlapAura)()
	  elseif Value == false then
	    _G.SlapAura = false
	  end
	end    
})
