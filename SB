Tab6:AddButton({
    Name = "Server Hop",
    Callback = function()
local screen = Instance.new('ScreenGui')
screen.Parent = game.Players.LocalPlayer.PlayerGui
--
local frame1 = Instance.new('Frame')
frame1.Parent = screen
frame1.Size = UDim2.new(0.5, 0, 0.7, 0)
frame1.Position = UDim2.new(0.25, 0, 0.13, 0)
frame1.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
frame1.BackgroundTransparency = 1
--
local corner = Instance.new('UICorner')
corner.Parent = frame1
corner.CornerRadius = UDim.new(0, 10)
--
local button1 = Instance.new('TextButton')
button1.Size = UDim2.new(0.4, 0, 0.2, 0)
button1.Position = UDim2.new(0.52, 0, 0.7, 0)
button1.Parent = frame1
button1.BackgroundColor3 = Color3.fromRGB(33, 87, 46)
button1.BackgroundTransparency = 0.2
button1.Text = 'Yes'
button1.TextSize = 15
button1.TextColor3 = Color3.fromRGB(255, 255, 255)
--
local bcorner = Instance.new('UICorner')
bcorner.CornerRadius = UDim.new(0, 5)
bcorner.Parent = button1
--
local button2 = Instance.new('TextButton')
button2.Parent = frame1
button2.Size = UDim2.new(0.4, 0, 0.2, 0)
button2.Position = UDim2.new(0.07, 0, 0.7, 0)
button2.Text = 'No'
button2.TextSize = 15
button2.BackgroundTransparency = 0.2
button2.TextColor3 = Color3.fromRGB(255, 255, 255)
button2.BackgroundColor3 = Color3.fromRGB(151, 48, 65)
--
local bcorner2 = Instance.new('UICorner')
bcorner2.Parent = button2
bcorner2.CornerRadius = UDim.new(0, 5)
--
local lbl = Instance.new('TextLabel')
lbl.BackgroundTransparency = 1
lbl.Size = UDim2.new(0.4, 0, 0.3, 0)
lbl.Text = 'Do You Want To Perform This Action ?'
lbl.TextColor3 = Color3.fromRGB(255, 255, 255)
lbl.TextSize = 16
lbl.Position = UDim2.new(0.3, 0, 0.27, 0)
lbl.Parent = frame1
--
local stroke = Instance.new('UIStroke')
stroke.Color = Color3.fromRGB(255, 255, 255)
stroke.Thickness = 2
stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
stroke.Parent = button1
stroke.Transparency = 1
--
local stroke2 = Instance.new('UIStroke')
stroke2.Color = Color3.fromRGB(255, 255, 255)
stroke2.Thickness = 2
stroke2.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
stroke2.Parent = button2
stroke2.Transparency = 1
--
button1.MouseButton1Down:Connect(function()
 stroke.Transparency = 0
stroke2.Transparency = 1  
end)
button1.MouseButton1Up:Connect(function()
  stroke.Transparency = 1
end)
button2.MouseButton1Down:Connect(function()
  stroke.Transparency = 1
  stroke2.Transparency = 0
end)
button2.MouseButton1Up:Connect(function()
  stroke2.Transparency = 1
end)
local tween121 = game:GetService('TweenService'):Create(frame1, TweenInfo.new(0.75), { ['BackgroundTransparency'] = 0 } )
tween121:Play()
button1.MouseButton1Click:Connect(function()
screen:Destroy()
local module = loadstring(game:HttpGet("https://raw.githubusercontent.com/LeoKholYt/roblox/main/lk_serverhop.lua"))()
        module:Teleport(game.PlaceId)
screen:Destroy()
end)
button2.MouseButton1Click:Connect(function()
screen:Destroy()
end)
    end
})
