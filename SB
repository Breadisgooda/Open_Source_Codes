    local leader = stop:FindFirstChild('leaderstats')
      if leader then
        local leader2 = leader:FindFirstChild('Glove')
        if leader2 and leader2.Value == 'STOP' then
        local hm2 = game.Players.LocalPlayer.Character:FindFirstChild('HumanoidRootPart')
          if hm2 then
            if hm2.Anchored == true then
              local table = {'Left Leg', 'Left Arm', 'Torso', 'Right Leg', 'Right Arm', 'Head', 'HumanoidRootPart'}
              local plr = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
              for i, v in pairs(table) do
                plr[v].Anchored = false
              end
            end
          end
        end
      end
    end
    task.wait()
  end
end
