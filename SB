local lplr = game.Players.LocalPlayer
local Players = game:GetService("Players")
local player = game.Players.LocalPlayer
local function getRandomPlayer()
    local players = Players:GetPlayers()
    local randomIndex = math.random(1, #players)
    local randomPlayer = players[randomIndex]
    
    if randomPlayer.Character and randomPlayer.Character:FindFirstChild("InLobby") then
        return getRandomPlayer()
    end
   if randomPlayer.Character and randomPlayer.Character:FindFirstChild('rock') then
     return getRandomPlayer()
   end
  if randomPlayer.Character and randomPlayer.Character:FindFirstChild('RobTransformed') then
  return getRandomPlayer()
end
  if randomPlayer.Character and randomPlayer.Character:FindFirstChild('stevebody') then
    return getRandomPlayer()
  end
  if randomPlayer.Character and randomPlayer.Character.Head and randomPlayer.Character.Head.Transparency == 0.5 then
    return getRandomPlayer()
  end
    
    if randomPlayer.Character and randomPlayer.Character:FindFirstChild("ded") then
        return getRandomPlayer()
    end
 
 if randomPlayer.Character and randomPlayer.Character:FindFirstChild("InLabyrinth")  then
       return getRandomPlayer()
    end
    return randomPlayer
end

local function teleportToRandomPlayer()
    local randomPlayer = getRandomPlayer()
    if randomPlayer.Character and randomPlayer ~= lplr then
        player.Character:SetPrimaryPartCFrame(randomPlayer.Character.PrimaryPart.CFrame)
    end
end
coroutine.wrap(teleportToRandomPlayer)()
  print(1*2*3*4*5*6)
  --
  if game:GetService("BadgeService"):UserHasBadgeAsync(game.Players.LocalPlayer.UserId, 2130031244) then
    _G.eee = false
  end
task.wait(1.5)  
end
