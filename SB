Tab5:AddDropdown({
	Name = "Anti Table",
	Default = "",
	Options = {"On", "Off"},
	Callback = function(Value)
		if Value == 'On' then
		  local pts = {'Head', 'Torso', 'Right Arm', 'Right Leg', 'Left Leg', 'Left Arm', 'HumanoidRootPart'}
		  _G.liker = true
local TableObject = workspace:FindFirstChild("TableObject")

while _G.liker do
    local tableObjects = {}
    for _, object in pairs(workspace:GetChildren()) do
        if object:IsA("BasePart") and object.Name:find("TableObject") then
            table.insert(tableObjects, object)
        end
    end
    for _, tableObject in pairs(tableObjects) do
        local function onTouch(otherPart)
            print('You Got Hit By '.. tableObject.Name .. " . who was the owner of that table ? ".. tableObject:WaitForChild('TableOwner').Value ..' was, get your revenge')
          for _, hitted in pairs(pts) do
            game.Players.LocalPlayer.Character[hitted].Anchored = true
            wait()
            game.Players.LocalPlayer.Character[hitted].Anchored = false
          end
        end
        tableObject.Touched:Connect(onTouch)
    end
    task.wait()
end
		elseif Value == 'Off' then
		  _G.liker = false
		end
	end    
})
