Tab5:AddDropdown({
	Name = "Anti Shard & Snow",
	Default = "",
	Options = {"On", "Off"},
	Callback = function(Value)
		if Value == 'On' then
		  local Players = game:GetService("Players")
_G.shord = true
local function checkForSnow()
  while _G.shord do
    for _, player in pairs(Players:GetPlayers()) do
        local leaderstats = player:FindFirstChild("leaderstats")
        if leaderstats then
            local items = leaderstats:FindFirstChild("Glove")
            if items and items.Value == 'Snow' or items and items.Value == 'Shard' then
                local hmn = game.Players.LocalPlayer.Character:FindFirstChild('Humanoid')
              if hmn then
                if hmn.WalkSpeed == 4 then
                  hmn.WalkSpeed = 20
                end
              end
            end
        end
    end
    task.wait()
  end
end

checkForSnow()
		elseif Value == 'Off' then
		  _G.shord = false
	end 
	end
