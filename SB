local function test()
  wait(1)
  _G.duck = true
local screen = Instance.new('ScreenGui')
  screen.Name = 'glitch'
screen.Parent = game.Players.LocalPlayer.PlayerGui
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0.1, 0, 0.2, 0)
frame.Position = UDim2.new(0.05, 0, 0.02, 0)
frame.Parent = screen
  frame.BackgroundTransparency = 1
local smallFrames = {}
while _G.duck do
for i = 1, 60 do
    local smallFrame = Instance.new("Frame")
    smallFrame.Size = UDim2.new(0, math.random(10, 40), 0, math.random(10, 40))
    smallFrame.Position = UDim2.new(0, math.random(0, 1000), 0, math.random(0, 1000))
    smallFrame.BackgroundColor3 = Color3.new(1, 1, 1)
    smallFrame.Parent = frame
    table.insert(smallFrames, smallFrame)
end wait()
    for i, smallFrame in ipairs(smallFrames) do
        smallFrame:Destroy()
    end
  if game.Players.LocalPlayer.PlayerGui:FindFirstChild('fill') == nil then
    _G.duck = false
  end
    wait()
end
end
coroutine.wrap(test)()
tweening2:Play()
wait(3)
music1:Play()
wait(1.5)
tweening3:Play()
talking:Play()
wait(3)
tweening4:Play()
talking:Play()
wait(1.5)
tweening5:Play()
fade:Play()
-- 
local function com()
  wait(1)
  local plr = game:GetService'Players'.LocalPlayer.Character or game:GetService'Players'.LocalPlayer.CharacterAdded:Wait()
    if math.random() < 0.5 then
        plr:WaitForChild('HumanoidRootPart').CFrame = CFrame.new(-3.87063646, -5.14444304, -215.480469, -0.981467426, 2.30212631e-08, 0.191629112, 2.06103739e-08, 1, -1.45742627e-08, -0.191629112, -1.03546167e-08, -0.981467426)
    else
        plr:WaitForChild('HumanoidRootPart').CFrame = CFrame.new(-7.15520287, -5.06729174, 221.930801, 0.932697892, -6.51104815e-09, -0.360658616, -2.41757531e-10, 1, -1.86784259e-08, 0.360658616, 1.75085191e-08, 0.932697892)
    end
  screen2:Destroy()
  game.Players.LocalPlayer.PlayerGui.glitch:Destroy()
end
tweening5.Completed:Connect(com)
    wait(2)
    _G.test = true
  end
end
part.Touched:Connect(test) 
end  
  	end    
})
