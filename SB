Tab1:AddDropdown({
	Name = "Replica slap farm updated《equip replica & use in lobby》",
	Default = "",
	Options = {"On", "Off"},
	Callback = function(Value)
		if Value == 'On' then
		  -- replica farm
_G.glob = true
local bpart = {'HumanoidRootPart', 'Head', 'Torso', 'Left Leg', 'Left Arm', 'Right Leg', 'Right Arm'}
local function useclone()
  while _G.glob do
    if game.Players.LocalPlayer.Character:FindFirstChild('InLobby') == nil then
    game:GetService('ReplicatedStorage').Duplicate:FireServer()
    end
    task.wait(20)
  end
end
coroutine.wrap(useclone)()
local function slapclone()
while _G.glob do
  for _, clone in pairs(workspace:GetChildren()) do
    if clone.Name:find(game.Players.LocalPlayer.Name) and clone:FindFirstChild('HumanoidRootPart') and clone ~= game.Players.LocalPlayer.Character then
      for _, clone2 in pairs(bpart) do
        clone:WaitForChild(clone2).Anchored = true
        if game.Players.LocalPlayer.Character:FindFirstChild('Default') or game.Players.LocalPlayer.Backpack:FindFirstChild('Default') then
        game:GetService('ReplicatedStorage').b:FireServer(clone:WaitForChild('HumanoidRootPart'))
        elseif game.Players.LocalPlayer.Character:FindFirstChild('Replica') or game.Players.LocalPlayer.Backpack:FindFirstChild('Replica') then
          game:GetService('ReplicatedStorage').ReplicaHit:FireServer(clone:WaitForChild('HumanoidRootPart'))
        end
      end
    end
  end
  task.wait()
end
end
coroutine.wrap(slapclone)()
local function tptoc()
  while _G.glob do
  if game.Players.LocalPlayer.Character:FindFirstChild('InLobby') then
    firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild('HumanoidRootPart'), workspace.Lobby.Teleport2, 0)
    firetouchinterest(game.Players.LocalPlayer.Character:WaitForChild('HumanoidRootPart'), workspace.Lobby.Teleport2, 1)
    wait(0.5)
    game.Players.LocalPlayer.Character:WaitForChild('HumanoidRootPart').CFrame = CFrame.new(-24036.2266, 316.696503, -855.74939, -0.432100594, 0.195096105, -0.880469501, -3.06623953e-08, 0.976319253, 0.216334686, 0.901825428, 0.0934783742, -0.421868116)
  elseif game.Players.LocalPlayer.Character:FindFirstChild('InLobby') == nil then
