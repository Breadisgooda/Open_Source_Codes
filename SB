  ['CorruptedVine'] = {'Wild Vine', 'Wild Vine', 'Wild Vine', 'Blood Rose', 'Dark Root', 'Elder Wood', 'Jade Stone'},
  ['Slow'] = {'Blue Crystal', 'Blue Crystal', 'Mushroom', 'Mushroom', 'Jade Stone', 'Plane Flower'},
  ['CorruptedField'] = {'Plane Flower', 'Plane Flower', 'Hazel Lily'}
}  
local function mix(name)
  local checkForTime = 0
  for i = 1, #PotList[name] do
  game.ReplicatedStorage:WaitForChild('AlchemistEvent'):FireServer(unpack({'MixItem', PotList[name][i]}))
    checkForTime = checkForTime + 1
  if checkForTime == #PotList[name] then
  game.ReplicatedStorage:WaitForChild('AlchemistEvent'):FireServer('BrewPotion')
  end
  end
end
local function start()
local mixList = {'grug', 'Nightmare', 'Confusing', 'Power', 'Paralyzing', 'Haste', 'Invisibility', 'Toxic', 'Freeze', 'Speed', 'Lethal', 'Feather', 'Antitoxin', 'CorruptedVine', 'Slow', 'CorruptedField', 'Invincible', 'Explosion'}
local PotCount = 0
for _, v4 in pairs(mixList) do
  mix(v4)
  PotCount = PotCount + 1
  Send('Info', PotCount ..' Added', 'rbxassetid://17105411523', 0.1)
end
end
local timer = tick()
start()
Send('Info', tick() - timer ..' secs', 'rbxassetid://17105411523', 5)
else
  Send('Info', 'you dont own alchemist', 'rbxassetid://17105411523', 5)
end      		
  	end    
})
