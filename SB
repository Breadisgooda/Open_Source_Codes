-- additional
local localplr = game:GetService('Players').LocalPlayer
-- moving somethings from replicatedstorage to the workspace
local repressedMemoriesMap = game:GetService('ReplicatedStorage'):FindFirstChild("RepressedMemoriesMap")
local repressedMemoriesNPC = game:GetService('ReplicatedStorage'):FindFirstChild("RepressedMemoriesNPC")
if repressedMemoriesMap and repressedMemoriesNPC then
  repressedMemoriesMap.Parent = workspace
  repressedMemoriesNPC.Parent = workspace
end
local retro1 = game:GetService('ReplicatedStorage'):FindFirstChild('Assets')
if retro1 then
local retro2 = retro1:FindFirstChild('Retro')
if retro2 then
local retro3 = retro2:FindFirstChild('Map')
if retro3 then
local retro4 = retro3:FindFirstChild('RetroObbyMap')
if retro4 then
  retro4.Parent = workspace
end
end
end
end
