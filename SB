Tab5:AddDropdown({
	Name = "Void Barrier",
	Default = "",
	Options = {"yes", "no"},
	Callback = function(Value)
if Value == "yes" then
local platform = Instance.new("Part")
platform.Size = Vector3.new(100000, 1, 100000) -- Set the size of the platform
platform.Position = Vector3.new(-4.08816528, -8.92293644, 1.83554244) -- Set the position
platform.Anchored = true -- Make the platform stay in place
platform.CanCollide = true -- Enable collision for the platform
platform.Transparency = 0.5 -- Set the transparency to make it glass-like
platform.Name = "dough" -- Change the name of the part to "dough"

-- Add the platform to the workspace
platform.Parent = Workspace
elseif Value == "no" then
local player = Workspace

local function removeDoughInstances(parent)
    for _, child in pairs(parent:GetChildren()) do
        if child.Name == "dough" then
            child:Destroy()
        else
            removeDoughInstances(child)
        end
    end
end

removeDoughInstances(player)
end
	end    
})
